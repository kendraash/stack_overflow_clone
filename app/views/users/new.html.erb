<h1>Sign Up</h1>
<%= render "layouts/errors", :object => @user %>

<%=form_for @user do |f| %>
  <%=f.label :username %>
  <%=f.text_field :username, :autofocus => 'autofocus' %>
  <%=f.label :email %>
  <%=f.text_field :email %>
  <%=f.label :password %>
  <%=f.password_field :password %>
  <%=f.label :password_confirmation %>
  <%=f.password_field :password_confirmation %>
  <%=f.submit "Sign Up" %>
<%end%>
